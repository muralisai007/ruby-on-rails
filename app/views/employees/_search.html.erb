
    <table class="table table-striped mt-3" id="employee">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>Date of Birth</th>
          <th>Aadhaar Input</th>
          <th>Designation</th>
          <th>Status</th>
          <th>Salary</th>
          <th>Total Annual Salary</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody >
        <% if @filtered_employees.present? %>
        <% @filtered_employees.each do |a|%>
       <tr>
        <td>
        <% a.first_name%>
        </td>
        </tr>
        <%else%>
        <%end%>
      </tbody>
    </table>

    
  



  
